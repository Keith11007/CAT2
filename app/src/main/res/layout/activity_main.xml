<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <Button
        android:id="@+id/sandwichbutton"
        android:layout_width="203dp"
        android:layout_height="148dp"
        android:text="Sandwich\n View Details"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="265dp"
        tools:layout_editor_absoluteY="488dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="253dp"
        android:layout_height="157dp"
        app:srcCompat="@drawable/sandwich"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="488dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="315dp"
        android:layout_height="142dp"
        app:srcCompat="@drawable/boiledeggs"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-43dp"
        tools:layout_editor_absoluteY="18dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="236dp"
        android:layout_height="141dp"
        app:srcCompat="@drawable/rice"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-1dp"
        tools:layout_editor_absoluteY="168dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="233dp"
        android:layout_height="141dp"
        app:srcCompat="@drawable/sausage"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="19dp"
        tools:layout_editor_absoluteY="309dp" />

    <Button
        android:id="@+id/boiledeggsbutton"
        android:layout_width="287dp"
        android:layout_height="123dp"
        android:text="Boiled Eggs\n View Details"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="217dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/ricebutton"
        android:layout_width="252dp"
        android:layout_height="128dp"
        android:text="Rice\n View Details"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="248dp"
        tools:layout_editor_absoluteY="171dp" />

    <Button
        android:id="@+id/sausagebutton"
        android:layout_width="203dp"
        android:layout_height="148dp"
        android:text="Sausages\n View Details"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="265dp"
        tools:layout_editor_absoluteY="301dp" />


</androidx.constraintlayout.widget.ConstraintLayout>